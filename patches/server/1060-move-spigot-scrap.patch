diff --git a/build.gradle.kts b/build.gradle.kts
index 9b3a6b336cb1344d4e74e0e4f7c50ffd1e1b8955..8722efc25db3b9141c90dffc07ff72dc4590267f 100644
--- a/build.gradle.kts
+++ b/build.gradle.kts
@@ -3,6 +3,7 @@ import java.time.Instant
 
 plugins {
     java
+    idea // Paper
     `maven-publish`
 }
 
@@ -189,6 +190,20 @@ fun TaskContainer.registerRunTask(
     block(this)
 }
 
+// Paper start
+sourceSets.main {
+    java {
+        exclude("excluded/**")
+    }
+}
+
+idea {
+    module {
+        excludeDirs.add(projectDir.resolve("src/main/java/excluded"))
+    }
+}
+// Paper end
+
 val runtimeClasspathWithoutVanillaServer = configurations.runtimeClasspath.flatMap { it.elements }
     .zip(configurations.vanillaServer.map { it.singleFile.absolutePath }) { runtime, vanilla ->
         runtime.filterNot { it.asFile.absolutePath == vanilla }
